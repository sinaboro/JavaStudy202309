package ex04;


//1차원배열
public class Exam01 {

	public static void main(String[] args) {
		
	      int[] myArr = new int[5]; 

	      for(int i=0; i<myArr.length; i++) {
//	    	  int num = (int)(Math.random()*100)+1;
	    	  myArr[i] = (int)(Math.random()*45)+1;
	      }
	    	  
	      
	      for(int i=0; i<myArr.length; i++)
	    	  System.out.println(myArr[i]);
		
	}
	
	
	

}









